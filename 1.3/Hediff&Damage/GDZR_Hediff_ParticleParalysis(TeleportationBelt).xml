  <HediffDef>
    <defName>GDZR_HEDIFF_ParticleParalysisTeleportationBelt</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>particle paralyze</label>
    <labelNoun>particle paralyze</labelNoun>
    <description>To avoid misassembly particles are held suspended as reconstruction completes.</description>
  
    <defaultLabelColor>(254,254,254)</defaultLabelColor>
  <!-- (229,57,57)-->  
   
    <priceImpact>false</priceImpact>
  
  <!--  <initialSeverity>1</initialSeverity>-->
    
    <chronic>true</chronic>
    <isBad>false</isBad>
    <tendable>false</tendable>
    <everCurableByItem>false</everCurableByItem>
    <keepOnBodyPartRestoration>true</keepOnBodyPartRestoration>
    <makesAlert>false</makesAlert>
  
    <comps>
      <li Class="HediffCompProperties_Effecter">
        <stateEffecter>GDZR_EFFECTER_ParticleParalysis</stateEffecter>
      </li>
      <li Class="HediffCompProperties_SelfHeal">
        <healAmount>18</healAmount>
      </li>
    </comps>
    
