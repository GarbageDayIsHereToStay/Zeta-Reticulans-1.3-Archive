  <BodyPartDef>
    <defName>GDZR_BODYPART_ZetaTelelectrics</defName>
    <label>psy-limbic exocrine gland</label>
    <hitPoints>6</hitPoints>
    <permanentInjuryChanceFactor>15</permanentInjuryChanceFactor>
    <delicate>true</delicate>
    <skinCovered>false</skinCovered>
    <socketed>false</socketed>
    <beautyRelated>true</beautyRelated>
    <canSuggestAmputation>true</canSuggestAmputation>
    
    <tags>
      <li>GDZR_BODYPARTTAG_TelelectricsSource</li>
    </tags>
    <hitChanceFactors>
      <li>
        <key>Blunt</key>
        <value>0.3</value>
      </li>
    </hitChanceFactors>
  </BodyPartDef>
 
<!--■■■■■■■■■■■■■■■■■■ 2) Mantis Parts ■■■■■■■■■■■■■■■■■■■■■■■
    ■■■ Part Groups ■■■
    ■ Right Claw Group ■-->
  <BodyPartGroupDef>
    <defName>GDZR_PARTGROUP_MantisClawRight_AttackTool</defName>
    <label>right claw</label>
    <listOrder>85</listOrder>
  </BodyPartGroupDef>
  <!--■ Left Claw Group ■-->
  <BodyPartGroupDef>
    <defName>GDZR_PARTGROUP_MantisClawLeft_AttackTool</defName>
    <label>left claw</label>
    <listOrder>75</listOrder>
  </BodyPartGroupDef>
  
  <!--■■■ Claw Base ■■■-->
  <BodyPartDef Name="ZR_BodyPart_Mantis_Claws_Base" Abstract="True">
    <hitPoints>20</hitPoints>
    <!-- <isSolid>true</isSolid> -->
    <frostbiteVulnerability>.05</frostbiteVulnerability>
    
    <!-- <amputateIfGeneratedInjuredChance>0.2</amputateIfGeneratedInjuredChance> -->
