    <beautyRelated>true</beautyRelated>
    <tags>
      <li>ManipulationLimbDigit</li>
    </tags>
  </BodyPartDef>
  
  <!--■ Right Claw ■-->
  <BodyPartDef ParentName="ZR_BodyPart_Mantis_Claws_Base">
    <defName>GDZR_BODYPART_MantisClawRight</defName>
    <label>right claw</label>
  </BodyPartDef>
  <!--■ Left Claw ■-->  
  <BodyPartDef ParentName="ZR_BodyPart_Mantis_Claws_Base">
